{
    "env": {
        "browser": false,
        "node": true,
        "es2022": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended",
        "standard"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "node/no-unsupported-features/es-syntax": "off"
    }
}
  